{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "Remove",
  "title": "",
  "type": "object",
  "properties": {
    "app": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Which app would you like to init Prisma?"
    },
    "prismaDirectory": {
      "type": "string",
      "alias": "p",
      "description": "A directory where the prisma schema is located",
      "default": "app/prisma"
    },
    "schemaName": {
      "type": "string",
      "alias": "s",
      "description": "Prisma schema file name",
      "default": "schema"
    },
    "clientOutput": {
      "type": "string",
      "description": "Prisma client output"
    }
  },
  "required": ["app"]
}
