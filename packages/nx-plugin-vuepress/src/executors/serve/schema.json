{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Serve executor",
  "description": "",
  "type": "object",
  "properties": {
    "root": {
      "type": "string"
    },
    "docsDir": {
      "type": "string",
      "default": "docs"
    },
    "configPath": {
      "type": "string",
      "default": "docs/.vuepress/config.ts"
    },
    "host": {
      "type": "string",
      "default": "localhost"
    },
    "port": {
      "type": "number",
      "default": 8080
    },
    "temp": {
      "type": "string"
    },
    "cache": {
      "type": "string"
    },
    "cleanTemp": {
      "type": "boolean",
      "default": false
    },
    "cleanCache": {
      "type": "boolean",
      "default": false
    },
    "open": {
      "type": "boolean",
      "default": true
    },
    "debug": {
      "type": "boolean",
      "default": false
    },
    "noWatch": {
      "type": "boolean",
      "default": false
    },
    "verbose": {
      "type": "boolean",
      "default": false
    }
  },
  "required": ["root"]
}
