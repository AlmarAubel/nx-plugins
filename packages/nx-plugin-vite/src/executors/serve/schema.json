{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Serve executor",
  "description": "",
  "type": "object",
  "properties": {
    "root": {
      "type": "string",
      "description": "vite project root"
    },
    "configFile": {
      "type": "string",
      "description": "vite config file path"
    },
    "watch": {
      "type": "boolean",
      "default": false
    },
    "port": {
      "type": "number",
      "default": 3000
    },
    "host": {
      "type": "boolean",
      "default": false
    },
    "https": {
      "type": "boolean",
      "default": false
    }
  },
  "required": ["configFile"]
}
