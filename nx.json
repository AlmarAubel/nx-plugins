      "tags": [],
      "implicitDependencies": ["nx-plugin-serverless"]
    },
    "nx-plugin-esbuild": {
      "tags": ["scope:nx"],
      "implicitDependencies": [
        "nx-plugin-devkit",
        "esbuild-plugin-decorator",
        "esbuild-plugin-node-externals",
        "esbuild-plugin-filesize",
        "esbuild-plugin-ignore",
        "esbuild-plugin-html",
        "esbuild-plugin-alias-path"
