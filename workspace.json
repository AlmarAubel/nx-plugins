    "node-playground": {
      "root": "e2e/node-playground",
      "sourceRoot": "e2e/node-playground/src",
      "projectType": "application",
      "targets": {
        "swc": {
          "executor": "nx-plugin-swc:build"
        },
        "esbuild-build": {
          "executor": "nx-plugin-esbuild:build",
          "options": {
            "main": "e2e/node-playground/src/main.ts",
            "outputPath": "e2e/node-playground/dist",
            "tsconfigPath": "e2e/node-playground/tsconfig.app.json",
            "failFast": false,
            "watch": true,
            "skipTypeCheck": false,
            "write": false,
            "bundle": true,
            "decoratorHandler": "tsc",
            "watchDir": "e2e/node-playground/src",
            "verbose": true
          },
          "configurations": {
            "production": {
              "alias": {
                "./environments/environment": "e2e/node-playground/src/environments/environment.prod.ts"
              }
            }
          }
        },
        "esbuild-serve": {
          "executor": "nx-plugin-workspace:node-serve",
          "options": {
            "buildTarget": "node-playground:esbuild-build"
          }
        },
        "esbuild-serve-prod": {
          "executor": "nx-plugin-workspace:node-serve",
          "options": {
            "buildTarget": "node-playground:esbuild-build:production"
          }
        },
        "esbuild-info": {
          "executor": "nx-plugin-esbuild:info",
          "options": {
            "buildTarget": "node-playground:esbuild-build",
            "serveTarget": "node-playground:esbuild-serve"
          }
        },
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/e2e/node-playground",
            "main": "e2e/node-playground/src/main.ts",
            "tsConfig": "e2e/node-playground/tsconfig.app.json",
            "assets": ["e2e/node-playground/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "e2e/node-playground/src/environments/environment.ts",
                  "with": "e2e/node-playground/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "node-playground:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["e2e/node-playground/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/e2e/node-playground"],
          "options": {
            "jestConfig": "e2e/node-playground/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
