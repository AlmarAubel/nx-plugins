{
  "cli": "nx",
  "id": "common-plugin-generator",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "importPath": {
      "type": "string",
      "alias": "i",
      "description": "Plugin import path"
    }
  },
  "required": ["name"]
}
