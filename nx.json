    },
    "esbuild-plugin-run": {
      "tags": []
    },
    "esbuild-plugin-svgr-import": {
      "tags": []
    },
    "esbuild-plugin-yaml-import": {
      "tags": []
    },
    "nx-plugin-devkit": {
      "tags": ["scope:nx", "scope:devkit"]
    },
    "nx-plugin-esbuild": {
      "tags": ["scope:nx"],
      "implicitDependencies": [
        "esbuild-plugin-decorator",
        "esbuild-plugin-node-externals",
        "esbuild-plugin-alias-path"
      ]
    },
    "nx-plugin-esbuild-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-plugin-esbuild"]
    },
    "nx-plugin-ice": {
