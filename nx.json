{
  "npmScope": "nps",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "nx.json": "*",
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "parallel": true,
        "maxParallel": 8,
        "skipNxCache": true
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "esbuild-plugin-alias-path": {
      "tags": []
    },
    "esbuild-plugin-clean": {
      "tags": []
    },
    "esbuild-plugin-compress": {
      "tags": []
    },
    "esbuild-plugin-copy": {
      "tags": []
