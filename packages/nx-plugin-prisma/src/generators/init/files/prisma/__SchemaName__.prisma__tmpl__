generator client {
  provider = "<%= ClientProvider %>"
  output   = "<%= ClientOutput %>"
}

datasource db {
  provider = "<%= DatasourceProvider _%>"
  <%# use unescaped tag to get env('DATABASE_URL') %>
  url      = <%- DatasourceURL %>
}

