      "nx-plugin-swc": ["packages/nx-plugin-swc/src/index.ts"],
      "nx-plugin-vite": ["packages/nx-plugin-vite/src/index.ts"],
      "nx-plugin-umi": ["packages/nx-plugin-umi/src/index.ts"],
      "nx-plugin-snowpack": ["packages/nx-plugin-snowpack/src/index.ts"],
      "nx-plugin-devkit": ["packages/nx-plugin-devkit/src/index.ts"],
      "nx-plugin-koa": ["packages/nx-plugin-koa/src/index.ts"],
      "nx-plugin-vitepress": ["packages/nx-plugin-vitepress/src/index.ts"],
      "nx-plugin-parcel": ["packages/nx-plugin-parcel/src/index.ts"],
