      "tags": []
    },
    "esbuild-plugin-node-externals": {
      "tags": []
    },
    "esbuild-plugin-run": {
      "tags": []
    },
    "node-playground": {
      "tags": []
    },
    "nx-plugin-devkit": {
      "tags": []
    },
    "nx-plugin-devkit-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-plugin-devkit"]
