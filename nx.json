    "esbuild-plugin-decorator": { "tags": ["scope:esbuild"] },
    "esbuild-plugin-node-externals": { "tags": ["scope:esbuild"] },
    "esbuild-plugin-filesize": { "tags": ["scope:esbuild"] },
    "esbuild-plugin-ignore": { "tags": ["scope:esbuild"] },
    "esbuild-plugin-html": { "tags": ["scope:esbuild"] },
    "esbuild-plugin-alias-path": { "tags": ["scope:esbuild"] },
    "vite-plugin-graphql": { "tags": [] },
    "esbuild-plugin-notifier": { "tags": [] },
