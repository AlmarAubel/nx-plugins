{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Develop executor",
  "description": "",
  "type": "object",
  "properties": {
    "cwd": {
      "type": "string"
    },
    "noBuild": {
      "type": "boolean",
      "default": false
    },
    "watchAdmin": {
      "type": "boolean",
      "default": true
    },
    "browser": {
      "oneOf": [{ "type": "string" }, { "type": "boolean" }],
      "default": "chrome"
    }
  },
  "required": ["cwd"]
}
