{
  "npmScope": "penumbra",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "nx-plugin-typegraphql": { "tags": [] },
    "nx-plugin-typegraphql-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-plugin-typegraphql"]
    },
    "nx-plugin-midway": { "tags": [] },
    "nx-plugin-midway-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-plugin-midway"]
    },
    "nx-plugin-prisma": { "tags": [] },
    "nx-plugin-prisma-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-plugin-prisma"]
    },
    "nx-plugin-serverless": { "tags": [] },
    "nx-plugin-serverless-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-plugin-serverless"]
    },
    "nx-plugin-esbuild": { "tags": [] },
    "nx-plugin-esbuild-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-plugin-esbuild"]
    }
  },
  "workspaceLayout": { "appsDir": "e2e", "libsDir": "packages" }
}
